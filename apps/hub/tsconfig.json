{
    "compilerOptions": {
      "rootDir": ".",
      "outDir": "build",
      "esModuleInterop": true,
      "module": "Node16",
      "moduleResolution": "node16",
      "resolveJsonModule": true,
      "lib": ["es2022"],
      "target": "es2022",
      "skipLibCheck": true,
      "strict": true,
      "noImplicitAny": true,
      "strictNullChecks": true,
      "noEmit": false
    },
    "include": [
      "src/**/*.ts",
      "test/**/*.ts"
  , "config.ts"  ]
  } 